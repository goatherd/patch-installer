    "composer-installer"
   ],
  "homepage": "https://github.com/goatherd/patch-installer",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Maik Penz",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=5.3"
  },
  "extra": {
     "class": "Goatherd\\Patch\\Installer"
  },
  "autoload": {
    "psr-0": {"Goatherd\\Patch\\": ""}
  }
}
